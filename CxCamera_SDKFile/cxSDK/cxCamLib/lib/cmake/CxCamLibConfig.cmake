include("${CMAKE_CURRENT_LIST_DIR}/CxCamLibTargets.cmake")

get_target_property(IMP_CONFIGS_PROP AT::CxCamLib IMPORTED_CONFIGURATIONS)

message("IMPORTED_CONFIGURATIONS: ${IMP_CONFIGS_PROP}")

list (FIND IMP_CONFIGS_PROP "DEBUG" _index)
get_target_property(IMPORTED_IMPLIB_VALUE AT::CxCamLib IMPORTED_IMPLIB_DEBUG)
get_target_property(IMPORTED_LOCATION_VALUE AT::CxCamLib IMPORTED_LOCATION_DEBUG)
if((_index EQUAL -1) OR ((NOT EXISTS "${IMPORTED_IMPLIB_VALUE}") AND (NOT EXISTS "${IMPORTED_LOCATION_VALUE}")))
	set_target_properties(AT::CxCamLib PROPERTIES
	  MAP_IMPORTED_CONFIG_DEBUG Release
	)
endif()
unset(IMPORTED_IMPLIB_VALUE)

